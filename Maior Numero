/******************************************************************************

Douglas - Lista 1 - Exercicio 1

*******************************************************************************/

#include <stdio.h>

int main() {
    int num1, num2, num3;

    printf("Digite três números inteiros: ");
    scanf("%d %d %d", &num1, &num2, &num3);

    if (num1 > num2 && num1 > num3) {
        printf("Posição 1\n");
    } else if (num2 > num1 && num2 > num3) {
        printf("Posição 2\n");
    } else {
        printf("Posição 3\n");
    }

    return 0;
}
